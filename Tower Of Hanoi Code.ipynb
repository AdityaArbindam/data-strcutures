    "def Towerofhanoi(n,a,b,c):\n",
    "    if(n==1):\n",
    "        print(a+\" \"+c)\n",
    "        return\n",
    "    Towerofhanoi(n-1,a,c,b)\n",
    "    print(a+\" \"+c)\n",
    "    Towerofhanoi(n-1,b,a,c)\n",
    "\n",
    "Towerofhanoi(3,'s','h','d')"
   ]
  }
